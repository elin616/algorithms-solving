   "source": [
    "cnt1, cnt2 = map(int,input().split())\n",
    "s = set()\n",
    "for _ in range(cnt1):\n",
    "    string1 = input()\n",
    "    s.add(string1)\n",
    "check = 0 \n",
    "for _ in range(cnt2):\n",
    "    string2 = input()\n",
    "    if string2 in s:\n",
    "        check += 1\n",
    "print (check)\n"
   ]
  }
